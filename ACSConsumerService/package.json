  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "export API_ENVR=1 && npm run clean && (conc --kill-others --raw npm:watch*)",
    "dev": "set API_ENVR=2 && npm run clean && (conc --kill-others --raw npm:watch*)",
    "testing": "set API_ENVR=3 && npm run clean && (conc --kill-others --raw npm:watch*)",
    "lnx": "export API_ENVR=2 && npm run clean && (conc --kill-others --raw npm:watch*)",
    "clean": "node -e \"const fs = require('fs'); fs.mkdirSync('build', { recursive: true }); fs.writeFileSync('build/bundle.js', '');\"",
    "build": "webpack --config=webpack.prod.js --color --progress",
    "watch-nodemon": "nodemon build/bundle.js --watch build",
    "watch-webpack": "webpack --config=webpack.dev.js --color --progress --watch"
  },
